{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": { 
    "react/jsx-uses-react": "off",   
    "react/jsx-uses-vars": "error",
    "react/react-in-jsx-scope": "off" }
}
